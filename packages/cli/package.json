{
  "name": "@fancn21th/unnamed-cli",
  "author": "AF",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fancn21th/unnamed-ui.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.1",
  "description": "CLI tool for unnamed-ui",
  "type": "module",
  "bin": {
    "unnamed-cli": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "prepublishOnly": "pnpm build",
    "release": "pnpm build && npm publish --access public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "unnamed-ui"
  ],
  "dependencies": {
    "chalk": "5.6.2",
    "commander": "^12.0.0",
    "cross-spawn": "7.0.6"
  },
  "devDependencies": {
    "@types/cross-spawn": "6.0.6",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
